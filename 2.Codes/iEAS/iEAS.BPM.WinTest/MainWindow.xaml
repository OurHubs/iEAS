        Title="MainWindow" Height="350" Width="525">
    <Canvas>
        <Button Name="btnStart" Content="启动流程" Canvas.Left="55" Canvas.Top="30" Width="75" Click="btnStart_Click"/>
        <TextBox Name="txtId" Height="23" Canvas.Left="225" TextWrapping="Wrap" Text="" Canvas.Top="33" Width="141" RenderTransformOrigin="0.924,0.422"/>
        <Label Content="当前Id" Canvas.Left="180" Canvas.Top="30" RenderTransformOrigin="-0.064,0.593"/>
        <Button Content="执行节点" Canvas.Left="55" Canvas.Top="95" Width="75" Click="Button_Click"/>
        <Label Content="SN" Canvas.Left="180" Canvas.Top="91"/>
        <TextBox Name="txtSN" Height="23" Canvas.Left="225" TextWrapping="Wrap" Text="" Canvas.Top="95" Width="120"/>
        <Label Content="User" Canvas.Left="180" Canvas.Top="131"/>
        <TextBox Name="txtUser" Height="23" Canvas.Left="225" TextWrapping="Wrap" Text="" Canvas.Top="135" Width="120"/>
    </Canvas>
</Window>
